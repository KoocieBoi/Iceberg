{
   "compilerOptions": {
      "baseUrl": ".",
      "paths": {
         "discord.js": [ "node_modules/discord.js/typings/index.d.ts" ]
      },
      "target": "esnext",
      "module": "commonjs",
      "outDir": "BotFiles-js",
      "resolveJsonModule": true,
      "esModuleInterop": true
   },
   "include": [
      "BotFiles-ts/**/*"
   ],
   "exclude": [
      "node_modules",
      "BotFiles-js",
      "Data"
   ]
}